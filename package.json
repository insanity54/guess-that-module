{
  "name": "guess-that-module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "npm run build && npm run deploy:ghpages && npm run deploy:neocities",
    "deploy:ghpages": "npx push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy:neocities": "bash src/util/neocitiesUpload.sh"
  },
  "dependencies": {
    "@jackens/md2json": "^1.6.14",
    "core-js": "^3.1.2",
    "globby": "^10.0.1",
    "json2md": "^1.6.5",
    "neocities": "0.0.3",
    "remark": "^11.0.1",
    "remark-parse": "^7.0.1",
    "remark-stringify": "^7.0.3",
    "unified": "^8.4.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-slider-component": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
